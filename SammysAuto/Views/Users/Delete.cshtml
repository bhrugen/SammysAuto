@model ApplicationUser
@{
    ViewData["Title"] = "Delete";
}

<br />
<h2>Delete User</h2>
<hr />

<form asp-action="Delete">
    <div class="form-border">
        <input type="hidden" asp-for="Id" />
        <fieldset disabled>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-2">
                        <label asp-for="FirstName" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="FirstName" class="form-control" />
                    </div>
                </div>

            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-2">
                        <label asp-for="LastName" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="LastName" class="form-control" />
                    </div>
                </div>

            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-2">
                        <label asp-for="Email" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Email" class="form-control" />
                    </div>
                </div>

            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-2">
                        <label asp-for="PhoneNumber" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="PhoneNumber" class="form-control" />
                    </div>
                </div>

            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-2">
                        <label asp-for="Address" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="Address" class="form-control" />
                    </div>
                </div>

            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-2">
                        <label asp-for="City" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="City" class="form-control" />
                    </div>
                </div>

            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-2">
                        <label asp-for="PostalCode" class="custom-label"></label>
                    </div>
                    <div class="col-sm-4">
                        <input asp-for="PostalCode" class="form-control" />
                    </div>
                </div>

            </div>
        </fieldset>
        <br />
        <div class="form-group">
            <input type="submit" value="Delete" class="btn btn-sm btn-danger" />
            <a asp-action="Index" class="btn btn-sm btn-success">Back to List</a>
        </div>
    </div>
</form>

